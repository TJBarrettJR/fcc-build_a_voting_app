{
  "name": "Voting-App",
  "version": "0.0.1",
  "description": "FCC Voting App",
  "main": "server.js",
  "scripts": {
    "start": "concurrently \"npm run build:watch\" \"npm run serve\"",
    "build": "tsc -p public/",
    "build:watch": "tsc -p public/ -w",
    "serve": "nodemon server.js",
    "lint": "tslint ./public/**/*.ts -t verbose"
  },
  "nodemonConfig": {
    "ignore": [
      "views/",
      "public/"
    ]
  },
  "dependencies": {
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/router": "^5.2.0",
    "body-parser": "^1.18.2",
    "core-js": "^2.5.3",
    "express": "^4.16.2",
    "express-partials": "^0.3.0",
    "express-session": "^1.15.6",
    "method-override": "^2.3.10",
    "mongoose": "^4.13.9",
    "morgan": "^1.9.0",
    "nodemon": "^1.14.11",
    "passport": "^0.4.0",
    "passport-github2": "^0.1.11",
    "router": "^1.3.2",
    "rxjs": "^5.5.6",
    "systemjs": "^0.20.19",
    "zone.js": "^0.8.20",
    "concurrently": "^3.5.1",
    "typescript": "^2.6.2",
    "tslint": "^5.9.1"
  },
  "engines": {
    "node": "8.x"
  },
  "repository": {
    "url": "https://github.com/TJBarrettJR/fcc-build_a_voting_app"
  },
  "license": "MIT",
  "keywords": [
    "node",
    "glitch",
    "express"
  ]
}
